<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tech.wd.ffecommerceproject">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".Application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RegisterActivity"/>
        <activity android:name=".activity.TotalActivity"/>
        <activity android:name=".activity.DetailActivity"/>
        <activity android:name=".activity.FootPrintActivity"/>
        <activity android:name=".activity.InformationActivity"/>
        <activity android:name=".activity.SubmitOrderActivity"/>
        <activity android:name=".activity.PublishComment"/>
        <activity android:name=".activity.MineAddressShowActivity"/>

    </application>

</manifest>